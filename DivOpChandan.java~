import java.util.*;

class DivOpChandan
{
    public static void main(String[] args)
    {
	Scanner scan = new Scanner(System.in);

	System.out.print("\n Enter the value of a : ");
	int a = scan.nextInt();  // a is divisor

	System.out.print("\n Enter the value of b : ");
	int b = scan.nextInt();  // b is dividend

	int i;
	
	for( i = 0; b * i < a; i++);
	System.out.print("\n After dividing " + a + " by " + b + " we get : " + i + "\n\n");
    }
}
